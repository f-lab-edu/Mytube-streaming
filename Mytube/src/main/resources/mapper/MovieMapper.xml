<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flab.Mytube.repository.MovieRepository">
    <insert id="save" useGeneratedKeys="true" keyProperty="MOVIE.id">
        INSERT INTO MOVIE(USER_ID, SUBJECT, URL)
        VALUES(#{movie.streamerId}, #{movie.subject}, #{movie.url});
    </insert>


</mapper>